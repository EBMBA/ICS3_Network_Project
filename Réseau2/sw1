enable 

conf t

hostname r2sw1

no ip domain-lookup

vlan 20
name Serveurs

vlan 30
name Administration

int f0/1
switchport mode trunk 
switchport trunk allowed vlan 20,30
no sh

int f0/2
switchport mode trunk 
switchport trunk allowed vlan 20,30
no sh

int f0/3
switchport mode access
switchport access vlan 20
no sh

int f0/4
switchport mode access
switchport access vlan 30
no sh